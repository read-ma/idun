{
  "name": "ReadMa",
  "version": "1.1.0",
  "description": "* Api in rails * Website in react.js",
  "keywords": [],
  "homepage": "https://beta.readma.com",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/readma-team/idun.git"
  },
  "browserslist": [
    "last 2 versions",
    "iOS > 7",
    "Safari > 7"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress --inline --content-base dist/",
    "serve:prod": "NODE_ENV=production webpack-dev-server -p --progress --inline --watch --content-base dist/",
    "build:production": "rm -rf dist && NODE_ENV=production webpack -p",
    "deploy:production": "yarn build:production",
    "postdeploy:production": "rsync -azP --delete dist/* $DEPLOY_USER@api.readma.com:domains/beta.readma.com/public_html",
    "deploy:staging": "yarn build:production",
    "postdeploy:staging": "rsync -azP --delete dist/* $DEPLOY_USER@api.readma.com:domains/staging.readma.com/public_html",
    "lint": "eslint src/ --ext .js --ext .jsx",
    "maintenance": "sh script/maintenance.sh",
    "unit-test": "jest",
    "e2e": "nightwatch --config nightwatch-local.conf.js",
    "e2eCI": "nightwatch --config nightwatch-saucelabs.conf.js --env chrome,firefox",
    "e2e-edge": "nightwatch --config nightwatch-saucelabs.conf.js --env edge"
  },
  "dependencies": {
    "chart.js": "^2.6.0",
    "classnames": "^2.2.5",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.1.1",
    "history": "^4.6.3",
    "lodash-es": "^4.17.4",
    "material-ui": "^0.14.4",
    "moment": "^2.18.1",
    "nightwatch": "^0.9.16",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-ga": "^2.2.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-binding": "^0.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "bell-on-bundler-error-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs": "^0.8.12",
    "file-loader": "^0.11.2",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^2.29.0",
    "jest-cli": "^14.1.0",
    "lru-memoize": "^1.0.2",
    "nightwatch-commands": "^3.0.0",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.5.3",
    "npmlog": "^4.1.2",
    "postcss": "^6.0.6",
    "postcss-calc": "^6.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-flexboxfixer": "^0.0.5",
    "postcss-focus": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-pseudoelements": "^5.0.0",
    "pre-push": "^0.1.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "request": "^2.81.0",
    "sass-loader": "^6.0.6",
    "selenium-download": "^2.0.10",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "svg-loader": "0.0.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.1",
    "webpack-md5-hash": "0.0.5",
    "workbox-webpack-plugin": "^1.1.0"
  },
  "jest": {
    "setupEnvScriptFile": "src/tests/env-scripts.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react"
    ]
  },
  "pre-push": [
    "lint"
  ]
}
