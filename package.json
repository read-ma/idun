{
  "name": "ReadMa",
  "version": "1.0.0",
  "description": "* Api in rails * Website in react.js",
  "keywords": [],
  "homepage": "https://beta.readma.com",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/readma-team/idun.git"
  },
  "scripts": {
    "start": "npm run dev",
    "build:dev": "rm -rf public && ./node_modules/.bin/webpack -p --config webpack.config.js",
    "build:production": "rm -rf dist && ./node_modules/.bin/webpack -p --config webpack.production.config.js",
    "lint": "./node_modules/.bin/eslint src/ --ext .js --ext .jsx",
    "test": "casperjs test test/test.js",
    "unit-test": "jest",
    "deploy:production": "npm run build:production",
    "postdeploy:production": "rsync -azP --delete dist/* $DEPLOY_USER@api.readma.com:domains/beta.readma.com/public_html",
    "deploy:staging": "npm run build:production",
    "postdeploy:staging": "rsync -azP --delete dist/* $DEPLOY_USER@api.readma.com:domains/staging.readma.com/public_html",
    "dev": "webpack-dashboard -- ./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base public/ --config webpack.config.js",
    "maintenance": "sh script/maintenance.sh"
  },
  "pre-push": [
    "lint"
  ],
  "dependencies": {
    "airbrake-js": "0.6.0",
    "axios": "^0.15.3",
    "chart.js": "^2.5.0",
    "history": "^4.5.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-ga": "^2.1.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.3.1",
    "redux-thunk": "^2.2.0",
    "material-ui": "^0.14.4",
    "classnames": "^2.2.5",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "casperjs": "^1.1.3",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "chokidar": "^1.6.1",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "fbjs": "^0.8.9",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^14.1.0",
    "lodash-webpack-plugin": "^0.11.2",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.5.0",
    "pre-push": "^0.1.1",
    "sass-loader": "^4.0.2",
    "request": "^2.79.0",
    "style-loader": "^0.13.2",
    "superagent": "^3.5.0",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.2",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^1.15.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "setupEnvScriptFile": "src/tests/env-scripts.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react"
    ]
  }
}
