{
  "name": "ReadMa",
  "version": "1.0.0",
  "description": "* Api in rails * Website in react.js",
  "keywords": [],
  "homepage": "http://beta.readma.com",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/readma-team/idun.git"
  },
  "scripts": {
    "start": "npm run dev",
    "build:production": "rm -rf dist && NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/ --ext .js --ext .jsx",
    "test": "casperjs test test/test.js",
    "unit-test": "jest",
    "deploy:production": "NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js",
    "postdeploy:production": "scp dist/* $DEPLOY_USER@logdock.com:domains/beta.readma.com/public_html",
    "deploy:staging": "NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js",
    "postdeploy:staging": "scp dist/* $DEPLOY_USER@logdock.com:domains/staging.readma.com/public_html",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --inline --content-base public/",
    "maintenance": "sh script/maintenance.sh"
  },
  "pre-push": [
    "lint"
  ],
  "dependencies": {
    "airbrake-js": "0.5.9",
    "axios": "^0.13.1",
    "chart.js": "^2.2.1",
    "history": "^3.0.0",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-ga": "^2.1.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "material-ui": "^0.14.4",
    "classnames": "^2.2.5",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.8",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "casperjs": "^1.1.3",
    "chokidar": "^1.6.0",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs": "^0.8.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jest-cli": "^14.1.0",
    "node-env-file": "^0.1.8",
    "node-sass": "^3.10.0",
    "pre-push": "^0.1.1",
    "request": "^2.74.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-browser-plugin": "^1.0.12",
    "webpack-dev-server": "^1.15.1",
    "webpack-manifest-plugin": "^1.0.1",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "setupEnvScriptFile": "src/tests/env-scripts.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react"
    ]
  }
}
